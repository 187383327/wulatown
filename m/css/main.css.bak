@charset "UTF-8";
p{margin:0;padding:0}
.navwrap{
	position:fixed;
	top:0px;
	width:100%;
	-moz-box-shadow:1px 1px 2px 0px rgba(0, 0, 0, 0.1);
	-webkit-box-shadow:1px 1px 2px 0px rgba(0, 0, 0, 0.1);
	box-shadow:1px 1px 2px 0px rgba(0, 0, 0, 0.1);
	background:rgba(255,255,255,0.95);
	z-index:999
}
.mynavbar{
    position: relative;
	min-height: 50px;
	margin:0 -15px;
}
.mynavbar-toggle{
    position: relative;
    float: right;
    margin: 15px 15px 0 0;
    background-color: transparent;
    background-image: none;
    border: 1px solid transparent;
    border-radius: 4px;
	}
.mynavbar-toggle .collapseBtn{
	width:21px;height:auto;
}
.navbar-title{
	float: left;
    height: 50px;
    padding: 15px 15px;
    font-size: 18px;
    line-height: 20px;
	color:#ffd62e;
	font-size:16px;
}
.navbar-title:hover
{ 
color:#ffd62e;
text-decoration: none;
}
.navbar-title span{
	color:#646464;
	font-size:15px;
}
.navbar-nav li a{
	color:#646464
}
.bottom_line{
	border-top:1px solid #e4e4e4;
}
.line7{
	background:#ecedf1;
	height:7px;
	width:100%;
}
.footer{
	border-top:1px solid #e4e4e4;
	padding:22px 0;
}
.footer p{
	color:#a4a4a4;
	text-align:center;
}
