@charset "UTF-8";
.banner-wrap{
	margin:50px -15px 0 -15px;
	clear:both;
}
.banner{
	position:absolute;	
	width:100%;
	height:auto;
}
.qp-wrap{
	position:relative;
	top:28px;
	right:14px;
	float:right;
	width:38%;
	height:auto;
}
.qp-wrap .qpbg{
	width:100%;
	height:auto;
}
.qp-content{
	position:absolute;
	top:0px;
	left:0px;
	width:100%; 
	height:100%;	
}
.wulatown-intro{
	margin:26% 12% 2% 12%;
	border-radius:2%;
	background:#f4f4f4;
	width:76%;
	height:auto;
	display:inline-block;
}
.wulatownIcon{
	width:34%;
	height:auto;
	float:left;
}
.ship-text-wrap{
	margin-top:2%;
	margin-left:4%;
	width:62%;
	overflow:hidden;
	float:left;
	padding-left:6px;
	}
.ship-title{
	color:#646464;
	font-size:12px;
}
.ship-exp{
	color:#898989;
	font-size:11px;
}
.qp-content a{
	color:#fff;
	line-height:30px;
	display:block;
	clear:both;
	margin:0 auto;
	
}
.town-download img{
	width:64%;
	height:auto;
	margin:0 18%;
}
.town-download span{
	position:absolute;
	left:0;
	top:2px
	width:100%;
}
.qt-wrap{
	position:relative;
	width:94%;
	float:right;
	margin-top:45%;
}
.qt-wrap .qtbg{
	position:relative;
	width:100%;
	height:auto;
}
.qt-content{
	position:absolute;
	top:0px;
	left:0px;
	width:41%; 
	height:66%;
}
.mod1-wrap{
	margin-top:50px;
}
.m1-title{
	color:#898989;
