	width:26%;
	height:auto;
}
.m1-centent{
	margin-top:14px;
	width:100%;
}
.m1-centent div{
	list-style-type:none;
	text-align:center;
	display:inline-block;
}
.m1-centent .paddingB{
	padding-bottom:20px;
}
.row-paddingB{
	font-size:14px;
	color:#646464;
}
.m1-centent div img{
	width:70%;
	height:auto;
	margin-bottom:7px;
}
